<?import javafx.scene.layout.VBox?>


<FlowPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="application.YoutubeSubscriberCountViewController">
   <children>
      <AnchorPane prefHeight="623.0" prefWidth="821.0">
         <children>
            <VBox layoutX="394.0" layoutY="47.0" prefHeight="521.0" prefWidth="374.0">
               <children>
                  <Label prefHeight="44.0" prefWidth="72.0" text="First Name" />
                  <TextField fx:id="fnameLabel" />
                  <Label prefHeight="37.0" prefWidth="70.0" text="Last Name" />
                  <TextField fx:id="lnameLabel" />
                  <Label prefHeight="46.0" prefWidth="158.0" text="Youtube Channel Name" />
                  <TextField fx:id="yChannelNameLabel" />
                  <Label prefHeight="46.0" prefWidth="150.0" text="Channel Creation Date" />
                  <DatePicker fx:id="channelDateLabel" onAction="#handleChannelDateLabel" onMouseClicked="#handleChannelDateLabelClicked" prefHeight="31.0" prefWidth="375.0" />
                  <Label prefHeight="38.0" prefWidth="171.0" text="Current Subscriber Count" />
                  <TextField fx:id="subCountLabel" />
                  <Label prefHeight="43.0" prefWidth="106.0" text="Channel Avater" />
                  <Button fx:id="chooseAvaterLabel" mnemonicParsing="false" onAction="#handleChooseAvaterLabel" onMouseClicked="#handleChooseAvaterLabelClicked" text="Choose Avater" />
                  <Button fx:id="saveLabel" mnemonicParsing="false" onAction="#handleSaveLabel" text="Save" />
                  <Button fx:id="clearFormLabel" mnemonicParsing="false" onAction="#handleClearFormLabel" text="Clear Form" />
               </children>
            </VBox>
            <ImageView fx:id="imageViewLabel" fitHeight="248.0" fitWidth="200.0" layoutX="53.0" layoutY="149.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
   </children>
</FlowPane>
