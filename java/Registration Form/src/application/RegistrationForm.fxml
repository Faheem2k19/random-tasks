<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<FlowPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RegistrationFormController">
   <children>
      <AnchorPane prefHeight="520.0" prefWidth="586.0">
         <children>
            <TextField fx:id="firstNameLabel" layoutX="200.0" layoutY="106.0" promptText="First Name" />
            <TextField fx:id="confirmPasswordLabel" layoutX="200.0" layoutY="328.0" promptText="Confirm Password" />
            <TextField fx:id="passwordLabel" layoutX="200.0" layoutY="270.0" promptText="Password" />
            <TextField fx:id="emailLabel" layoutX="200.0" layoutY="215.0" promptText="Email" />
            <TextField fx:id="lastNameLabel" layoutX="200.0" layoutY="160.0" promptText="Last Name" />
            <Button layoutX="168.0" layoutY="410.0" mnemonicParsing="false" onAction="#saveClicked" text="Save" />
            <Button layoutX="355.0" layoutY="410.0" mnemonicParsing="false" onAction="#clearFormClicked" text="Clear Form" />
            <Text layoutX="198.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Registration Form">
               <font>
                  <Font name="Calibri" size="26.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </children>
</FlowPane>
